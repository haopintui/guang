<view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],['itemData']]]]]}}" class="item" bindtap="__e"><view class="pic"><image src="{{itemData.goods.pic_url}}"></image></view><view class="content"><view class="item-box"><text class="title">{{itemData.goods.title}}</text></view><view class="item-box"><block wx:if="{{itemData.goods.weight>0}}"><view class="weight">热销指数<text class="em">{{itemData.goods.weight}}</text></view></block></view><view class="item-box"><view class="price">{{itemData.goods.user_type_name+" ¥"+itemData.price.price}}</view></view><block wx:if="{{itemData.goods.coupon_money>0}}"><view class="item-box"><view class="coupon-money">{{"￥"+itemData.coupon.coupon_money+"元券"}}</view></view></block><view class="item-box"><view class="coupon-price">券后价 ¥<text class="em">{{itemData.price.buy_price}}</text></view><view class="qiang-btn">立即抢</view></view></view></view>