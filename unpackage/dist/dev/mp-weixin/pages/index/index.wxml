<view class="container"><bottom-menus vue-id="1" menus-index="0" bind:__l="__l"></bottom-menus><view class="tui-header"><view class="tui-category" hover-class="opcity" hover-stay-time="{{150}}" data-event-opts="{{[['tap',[['classify',['$event']]]]]}}" bindtap="__e"><image class="icon" src="https://img.youdanhui.cn/cms_img/2019-09-06/5d71bf8337cd3.png"></image><view class="tui-category-scale">分类</view></view><view class="tui-rolling-search"><icon type="search" size="{{13}}" color="#999"></icon><swiper class="tui-swiper" vertical autoplay circular interval="8000"><block wx:for="{{hotSearch}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="tui-swiper-item" bindtap="__e"><view class="tui-hot-item">{{item}}</view></swiper-item></block></swiper></view></view><block wx:for="{{components}}" wx:for-item="item" wx:for-index="index" wx:key="index"><component-item vue-id="{{'2-'+index}}" item-data="{{item}}" bind:__l="__l"></component-item></block><view class="tui-product-box tui-pb-20 tui-bg-white"><view data-event-opts="{{[['tap',[['more',['$event']]]]]}}" class="tui-group-name" bindtap="__e"><text>新人专享</text><tui-icon vue-id="3" name="arrowright" size="{{20}}" color="#555" bind:__l="__l"></tui-icon></view><view data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" class="tui-activity-box" bindtap="__e"><image class="tui-activity-img" src="https://img.youdanhui.cn/cms_img/2019-09-06/5d71cd57c3c0e.jpg" mode="widthFix"></image><image class="tui-activity-img" src="https://img.youdanhui.cn/cms_img/2019-09-06/5d71cd2b3fc47.jpg" mode="widthFix"></image></view></view><view class="tui-product-box tui-pb-20 tui-bg-white"><view data-event-opts="{{[['tap',[['more',['$event']]]]]}}" class="tui-group-name" bindtap="__e"><text>新品推荐</text><tui-icon vue-id="4" name="arrowright" size="{{20}}" color="#555" bind:__l="__l"></tui-icon></view><view class="tui-new-box"><block wx:for="{{newProduct}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" class="{{['tui-new-item',index!=0&&index!=1?'tui-new-mtop':'']}}" bindtap="__e"><block wx:if="{{item.isLabel}}"><image class="tui-new-label" src="{{'../../../static/images/mall/new/'+(item.type==1?'new':'discount')+'.png'}}"></image></block><view class="tui-title-box"><view class="tui-new-title">{{item.name}}</view><view class="tui-new-price"><text class="tui-new-present">{{"￥"+item.present}}</text><text class="tui-new-original">{{"￥"+item.original}}</text></view></view><image class="tui-new-img" src="https://img.youdanhui.cn/cms_img/2019-09-06/5d71cd6f2de42.jpg"></image></view></block></view></view><view class="tui-product-box"><view class="tui-group-name"><text>热门推荐</text></view><view class="tui-product-list"><view class="tui-product-container guess-section"><block wx:for="{{items}}" wx:for-item="item" wx:for-index="index" wx:key="index"><goods vue-id="{{'5-'+index}}" item-data="{{item}}" goods-type="column" bind:__l="__l"></goods></block></view></view></view><tui-loadmore vue-id="6" visible="{{loadding}}" index="{{3}}" type="red" bind:__l="__l"></tui-loadmore><view class="tui-safearea-bottom"></view><tui-scroll-top vue-id="7" scrollTop="{{scrollTop}}" bind:__l="__l"></tui-scroll-top></view>