<view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],['itemData']]]]]}}" class="item" bindtap="__e"><view class="pic"><image src="{{itemData.goods.pic_url}}"></image></view><view class="content"><view class="item-box"><text class="title">{{itemData.goods.title}}</text></view><block tt:if="{{itemData.coupon.coupon_money>0}}"><view class="item-box"><view class="coupon-money">{{"￥"+itemData.coupon.coupon_money+"元券"}}</view></view></block><view class="item-box"><text>{{itemData.goods.user_type_name+" ¥"+itemData.price.buy_price}}</text><text>{{"已售 "+itemData.goods.volume_str}}</text></view></view></view>