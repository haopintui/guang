<view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],['itemData']]]]]}}" class="item" bindtap="__e"><view class="pic"><image src="{{itemData.goods.pic_url}}"></image></view><view class="content"><view class="item-box"><text class="title">{{itemData.goods.title}}</text></view><view class="item-box"><block tt:if="{{itemData.goods.juhuasuan>0}}"><view class="tqg">聚划算</view></block><block tt:if="{{itemData.goods.taoqinggou>0}}"><view class="tqg">淘抢购</view></block><text class="volume">{{"已售 "+itemData.goods.volume_str}}</text></view><view class="item-box"><view class="coupon-price">折后<text class="em">{{"¥"+itemData.price.buy_price}}</text></view><block tt:if="{{itemData.coupon.coupon_money>0}}"><view class="coupon-money">{{"￥"+itemData.coupon.coupon_money+"元券"}}</view></block></view></view></view>