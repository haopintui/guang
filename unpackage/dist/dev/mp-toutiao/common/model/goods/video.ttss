.floor-item {
  width: 180rpx;
  margin-right: 20rpx;
  font-size: 26rpx;
  color: #303133;
  line-height: 1.8;
}
.floor-item .img {
    width: 180rpx;
    height: 180rpx;
    border-radius: 6rpx;
    position: relative;
}
.floor-item .img .pic {
      width: 180rpx;
      height: 180rpx;
      border-radius: 6rpx;
      top: 0rpx;
      left: 0rpx;
      position: absolute;
}
.floor-item .img .play {
      width: 36rpx;
      height: 36rpx;
      position: absolute;
      top: 90rpx;
      left: 90rpx;
}
.floor-item .price {
    color: #fa436a;
}

