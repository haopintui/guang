<view class="tui-tabbar"><block tt:for="{{items}}" tt:for-item="item" tt:for-index="index" tt:key="index"><block><view class="{{(('tui-tabbar-item')+' '+(menusIndex==index?'tui-item-active':''))}}" data-index="{{index}}" data-event-opts="{{[['tap',[['tabbarSwitch',['$0'],[[['items','',index]]]]]]]}}" bindtap="__e"><view class="{{(index==0?'tui-ptop-4':'')}}"><image class="icon" src="{{menusIndex==index?item.images_active:item.images}}"></image></view><view class="tui-scale">{{item.text}}</view></view></block></block></view>