<view class="container"><view class="input-content"><view class="input-item"><text class="tit">账号:</text><input type="text" placeholder="输入支付宝账号" data-key="alipay" data-event-opts="{{[['input',[['inputChange',['$event']]]]]}}" value="{{alipay}}" bindinput="__e"/></view><view class="input-item"><text class="tit">收款人姓名:</text><input type="text" placeholder="输入支付宝真实用户名" placeholder-class="input-empty" data-key="payee_name" data-event-opts="{{[['input',[['inputChange',['$event']]]]]}}" value="{{payee_name}}" bindinput="__e"/></view></view><button class="confirm-btn" disabled="{{logining}}" data-event-opts="{{[['tap',[['update',['$event']]]]]}}" bindtap="__e">设置</button></view>